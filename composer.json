{
    "name": "bobroid/yii2-rss",
    "description": "Yii2 RSS extension adds RSS-feed to your site",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "rss",
        "feed"
    ],
    "homepage": "https://github.com/zelenin/yii2-rss",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Developer"
        },
        {
            "name": "Nikolai Gilko",
            "email": "n.gilko@gmail.com",
            "role": "added some features"
        }
    ],
    "support": {
        "issues": "https://github.com/zelenin/yii2-rss/issues",
        "source": "https://github.com/zelenin/yii2-rss"
    },
    "require": {
        "yiisoft/yii2": "~2",
        "bobroid/rss-generator": "*"
    },
    "repositories": [
        {
            "type": "git",
